{
  "rewrites": [
    {
      "source": "/restaurants-by-cuisine/:path*",
      "destination": "https://restuarnt-finder.vercel.app/restaurants-by-cuisine/:path*"
    },
    {
      "source": "/api/analyze-image/:path*",
      "destination": "https://restuarnt-finder.vercel.app/api/analyze-image/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "VITE_API_URL": "https://restuarnt-finder.vercel.app/"
  }
} 